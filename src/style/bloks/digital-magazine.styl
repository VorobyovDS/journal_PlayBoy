.digital-magazine {
  display flex
  justify-content center
  align-items center
  color #fff
  font-family "Roboto", sans-serif
  font-size 12px
  line-height 1.2
  &__img {
    width 45px
    height 45px
  }
  &__text {
    max-width 100px
    padding-left 11px
    &-wr {
      background-color #333333
      display flex
      align-items center
      width 136px
      height 45px
    }
  }
}